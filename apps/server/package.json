{
  "name": "server",
  "version": "0.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "pnpm start:dev",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:staging": "cp .env.staging .env && nest start --watch",
    "start:prod": "cp .env.production .env && node dist/main",
    "prod": "pnpm start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prestart": "rimraf dist && pnpm build",
    "prisma:generate": "npx prisma generate",
    "db:migrate:new": "npx prisma migrate dev --name",
    "db:migrate": "npx prisma migrate dev && npx prisma generate",
    "db:seed": "npx ts-node -r tsconfig-paths/register prisma/seed.ts",
    "db:reset": "npx prisma migrate reset; pnpm db:seed",
    "prepare": "ts-patch install -s"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.10.3",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "5.10.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.2",
    "@trpc/server": "^10.45.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "ejs": "^3.1.9",
    "generate-password-ts": "^1.6.5",
    "glob": "^10.3.10",
    "joi": "^17.12.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "random-avatar-generator": "^2.0.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
